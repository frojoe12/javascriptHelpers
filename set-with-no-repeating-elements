<script>
  const arraySet = () => {
    items = []
    isAlreadyIn = newItem => {
      const isIn = items.includes(newItem) ? true : false
      return isIn
    }
    addItem = item => {
      !isAlreadyIn(item) ? (items = [...items, item]) : null
    }
    displaySet = () => {
      console.log(items)
    }
    return {
      addItem,
      displaySet
    }
  }

  const myItems = arraySet()

  myItems.addItem('Apple')
  myItems.addItem('Banana')
  myItems.addItem('Orange')
  myItems.addItem('Apple')
  myItems.addItem('Apple')
  myItems.addItem('Strawberry')
  myItems.addItem('Strawberry')
  myItems.addItem('Strawberry')
  myItems.addItem('Strawberry')
  myItems.addItem('Strawberry')
  myItems.addItem('Strawberry')

  myItems.displaySet()
</script>
