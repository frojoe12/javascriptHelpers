<script>
  const arraySet = () => {
    items = []
    isInSet = newItem => {
      const isIn = items.includes(newItem) ? true : false
      return isIn
    }
    addItem = item => {
      !isInSet(item) ? (items = [...items, item]) : null
    }
    displaySet = () => {
      console.log(items)
    }
    return {
      addItem,
      displaySet,
      isInSet
    }
  }

  const myItems = arraySet()

  myItems.addItem('Apple')
  myItems.addItem('Banana')
  myItems.addItem('Orange')
  myItems.addItem('Apple')
  myItems.addItem('Apple')
  myItems.addItem('Strawberry')
  myItems.addItem('Strawberry')
  myItems.addItem('Strawberry')
  myItems.addItem('Strawberry')
  myItems.addItem('Strawberry')
  myItems.addItem('Strawberry')

  console.log(`Peach is in set = ${myItems.isInSet('Peach')}`)
  console.log(`Apple is in set = ${myItems.isInSet('Apple')}`)

  myItems.displaySet()
</script>
